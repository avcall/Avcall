{
	"name": "avcall",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"start": "node node_modules/react-native/local-cli/cli.js start",
		"android": "node node_modules/react-native/local-cli/cli.js run-android",
		"ios": "node node_modules/react-native/local-cli/cli.js run-ios",
		"make-icon": "node node_modules/app-icon/bin/app-icon.js generate",
		"test": "jest",
		"build": "tsc"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/avcall/Avcall.git"
	},
	"dependencies": {
		"firebase": "^4.1.3",
		"react": "16.0.0-alpha.12",
		"react-native": "^0.46.0",
		"react-native-gifted-chat": "^0.2.0",
		"react-native-tabs": "^1.0.9",
		"react-navigation": "^1.0.0-beta.11"
	},
	"devDependencies": {
		"@types/jest": "^20.0.2",
		"@types/react": "^15.0.34",
		"@types/react-native": "^0.44.15",
		"@types/react-navigation": "^1.0.15",
		"@types/react-test-renderer": "^15.5.1",
		"app-icon": "^0.6.0",
		"babel-jest": "20.0.3",
		"babel-preset-react-native": "2.0.0",
		"jest": "20.0.4",
		"react-test-renderer": "16.0.0-alpha.12",
		"ts-jest": "^20.0.6",
		"typescript": "^2.4.1"
	},
	"jest": {
		"preset": "react-native",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js"
		],
		"transform": {
			"^.+\\.js$": "<rootDir>/node_modules/babel-jest",
			".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
		},
		"testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
		"testPathIgnorePatterns": [
			"\\.snap$",
			"<rootDir>/node_modules/",
			"<rootDir>/lib/"
		],
		"cacheDirectory": ".jest/cache"
	}
}
